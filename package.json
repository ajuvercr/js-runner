{
  "name": "@ajuvercr/js-runner",
  "version": "0.1.13",
  "type": "module",
  "description": "",
  "main": "./dist/index.js",
  "files": [
    "./dist/**/*",
    "./ontology.ttl",
    "./processor/**/*",
    "./channels/**/*"
  ],
  "types": "./dist/index.d.ts",
  "bin": {
    "js-runner": "./bin/bundle.mjs"
  },
  "scripts": {
    "build": "tsc && bun build ./bin/js-runner.js --outfile bin/bundle.mjs --target node && npm run build:recompose",
    "build:recompose": "sed -z 's/var __require = (id) => {\\n  return import.meta.require(id);\\n};/import Module from \"node:module\";\\nconst __require = Module.createRequire(import.meta.url);/' -i bin/bundle.mjs",
    "watch": "tsc -w",
    "test": "bun test"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rdfjs/types": "^1.1.0",
    "@treecg/types": "^0.4.5",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "kafkajs": "^2.2.4",
    "n3": "^1.17.1",
    "rdf-lens": "^0.0.4",
    "stream-to-array": "^2.3.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/command-line-args": "^5.2.2",
    "@types/command-line-usage": "^5.0.3",
    "@types/n3": "^1.16.3",
    "@types/node": "^18.11.15",
    "@types/ws": "^8.5.8",
    "typescript": "^5.2.2"
  }
}
