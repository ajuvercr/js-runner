[ ] a :Channel;
  :reader <jr>;
  :writer <jw>.
<jr> a js:JsReaderChannel.
<jw> a js:JsWriterChannel.

[ ] a js:Send;
  js:msg "Hallo world";
  js:sendWriter <jw>.

[ ] a js:Resc;
  js:rescReader <jr>.

