@prefix js: <https://w3id.org/conn/js#> .
@prefix ws: <https://w3id.org/conn/ws#> .
@prefix : <https://w3id.org/conn#> .

[] a js:JsChannel;
  :reader <jr>;
  :writer <jw>.

<jr> a :JsReaderChannel.
<jw> a :JsWriterChannel.

[] a :WsChannel;
  :reader :WsReaderChannel;
  :writer :WsWriterChannel.

<wsReader> a :WsReaderChannel;
  :wsPort "3000".

<wsWriter> a :WsWriterChannel;
  :wsUri "ws://localhost:3000".

[] a js:Send;
  js:msg "Hallo world";
  js:sendWriter <wsWriter>.

[] a js:Resc;
  js:rescReader <wsReader>.

